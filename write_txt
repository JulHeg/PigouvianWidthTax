from numpy.random import randint
from time import sleep


while True:
    sleep(5)
    with open('coordinates.txt', 'w') as f:
        #lines = f.readlines()
        
        x = str(randint(50, 600, 1))
        print(x)
        f.write(x)
        f.close()
