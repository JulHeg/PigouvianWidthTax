"""

from numpy.random import randint
from time import sleep
import datetime as dt


while True:
    sleep(10)
    with open('shift.txt', 'w') as f:
        
        x = str([randint(200, 450, 1)[0], dt.datetime.now().strftime("%H:%M:%S")])[1:-1]
        print(x)
        f.write(x)
        f.close()
